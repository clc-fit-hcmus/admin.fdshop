<!--**********************************
    Content body start
***********************************-->
<div class="content-body">
    <div class="container-fluid">
        <div class="row page-titles mx-0">
            <div class="col-sm-6 p-md-0">
                <div class="welcome-text">
                    <h4>Hi, welcome back!</h4>
                    <p class="mb-0">Your business dashboard template</p>
                </div>
            </div>
            <div class="col-sm-6 p-md-0 justify-content-sm-end mt-2 mt-sm-0 d-flex">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="javascript:void(0)">Layout</a></li>
                    <li class="breadcrumb-item active"><a href="javascript:void(0)">Blank</a></li>
                </ol>
            </div>
        </div>
        <div class="row">

            {{#each fds}}
                <div class="col-lg-12 col-xl-6">
                    <div class="card">
                        <div class="card-body">
                            <div class="row m-b-30">
                                <a href="/product-details/{{this._id}}">
                                    <div class="col-md-5 col-xxl-12">
                                        <div class="new-arrival-product mb-4 mb-xxl-4 mb-md-0">
                                            <div class="new-arrivals-img-contnent">
                                                <img class="img-fluid" src={{this.avatar}} alt={{this.cloudinary_id}}>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                                <div class="col-md-7 col-xxl-12">
                                    <div class="new-arrival-content position-relative">
                                        <a href="/product-details/{{this._id}}" class="a-cover"><h4>{{this.name}}</h4></a>
                                        <p class="price">${{this.price}}</p>
                                        <p>Availability: <span class="item"> In stock <i
                                                    class="fa fa-check-circle text-success"></i></span></p>
                                        <p>Product code: <span class="item">{{this._id}}</span> </p>
                                        <p>Brand: <span class="item">Lee</span></p>
                                        <p class="text-content">{{this.description}}</p>
                                        <div class="comment-review star-rating text-right">
                                            <ul>
                                                <li><i class="fa fa-star"></i></li>
                                                <li><i class="fa fa-star"></i></li>
                                                <li><i class="fa fa-star"></i></li>
                                                <li><i class="fa fa-star-half-empty"></i></li>
                                                <li><i class="fa fa-star-half-empty"></i></li>
                                            </ul>
                                            <span class="review-text">(34 reviews) / </span><a class="product-review" href="">Write a review?</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>

        {{#if is_notOne}}

        <nav aria-label="Page navigation example" style="height: 100px;">
            <ul class="pagination justify-content-center">

                {{#if prev}}
                
                <li class="page-item">
                    <a class="page-link" href="/product-list?page={{prev}}" tabindex="-1">Previous</a>
                </li>

                {{/if}}

                {{#times pages}}

                <li class="page-item"><a class="page-link" href="/product-list?page={{this}}">{{this}}</a></li>

                {{/times}}

                {{#if is_overload}}{{else}}

                <li class="page-item">
                    <a class="page-link" href="/product-list?page={{next}}">Next</a>
                </li>

                {{/if}}
            </ul>
        </nav>

        {{/if}}
    </div>
</div>
<!--**********************************
    Content body end
***********************************-->